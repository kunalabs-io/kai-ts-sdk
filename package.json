{
  "name": "@kunalabs-io/kai",
  "version": "0.16.0",
  "license": "Apache-2.0",
  "author": "Kuna Labs <build@kunalabs.io>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunalabs-io/kai-ts-sdk.git"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "pnpm clean && pnpm tsup ./src/index.ts --format esm,cjs --sourcemap --dts --tsconfig tsconfig.build.json",
    "watch": "pnpm build --watch",
    "check": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@cetusprotocol/aggregator-sdk": "0.12.1",
    "@mysten/sui": "1.28.2",
    "@opentelemetry/api": "1.9.0",
    "@pythnetwork/pyth-sui-js": "^2.1.0",
    "@sinclair/typebox": "^0.33.22",
    "aftermath-ts-sdk": "^1.3.12",
    "bn.js": "^5.2.1",
    "decimal.js": "^10.4.3",
    "lru-cache": "^11.0.2",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.48.1",
    "@types/bn.js": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "5.6.3"
  }
}
